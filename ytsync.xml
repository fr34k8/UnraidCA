<?xml version="1.0"?>
<Container version="2">
  <Name>YTSync</Name>
  <Repository>pickl3s/ytsync:latest</Repository>
  <Registry>https://hub.docker.com/r/pickl3s/ytsync</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/PugPickles/UnraidCA/tree/YTSync</Project>
  <Overview>Sync youtube playlists locally, files can be downloaded as mp3 or mp4.&#xD;
Practical for e.g. Jellyfin</Overview>
  <Category>Downloaders:</Category>
  <WebUI>http://[IP]:[PORT:8095]</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/PugPickles/UnraidCA/0845d6f76ff45a020706ee6d374d7b5327aac8df/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1667667777</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Media root dir." Target="/MEDIA_ROOT" Default="" Mode="rw" Description="The main directory where the folders of the media (music, videos) are located" Type="Path" Display="always" Required="true" Mask="false"/>
  <Config Name="WebUI Port" Target="8095" Default="8095" Mode="tcp" Description="The port through which the website is accessible" Type="Port" Display="advanced" Required="false" Mask="false">8095</Config>
  <Config Name="WebUI Port" Target="WEB_PORT" Default="8095" Mode="" Description="Change port for website" Type="Variable" Display="advanced" Required="false" Mask="false">8095</Config>
  <Config Name="API Port" Target="40123" Default="40123" Mode="tcp" Description="The port through which the API is accessible" Type="Port" Display="advanced" Required="false" Mask="false">40123</Config>
  <Config Name="API Port" Target="API_PORT" Default="40123" Mode="" Description="Change port for API" Type="Variable" Display="advanced" Required="false" Mask="false">40123</Config>
</Container>
